{
  "name": "@matstack/react-adonisjs",
  "description": "An adapter for using React Router with AdonisJS",
  "version": "1.0.6",
  "engines": {
    "node": ">=20.10.0"
  },
  "main": "build/index.js",
  "type": "module",
  "files": [
    "src",
    "providers",
    "index.ts",
    "build/commands",
    "build/src",
    "build/providers",
    "build/stubs",
    "build/index.d.ts",
    "build/index.d.ts.map",
    "build/index.js",
    "build/configure.d.ts",
    "build/configure.d.ts.map",
    "build/configure.js"
  ],
  "exports": {
    ".": "./build/index.js",
    "./commands": "./build/commands/main.js",
    "./react_router_provider": "./build/providers/react_router_provider.js",
    "./build_hook": "./build/src/hooks/build_hook.js",
    "./types": "./build/src/types/main.js"
  },
  "scripts": {
    "build": "pnpm run compile",
    "clean": "del-cli build",
    "compile": "pnpm run clean && tsc",
    "dev": "tsc --watch --preserveWatchOutput",
    "copy:templates": "copyfiles \"stubs/**/**/*.stub\" build",
    "copy:readme": "copyfiles -u 2 \"../../*.md\" build",
    "format": "prettier --write .",
    "lint": "eslint . --ext=.ts",
    "postcompile": "pnpm run copy:templates && pnpm run copy:readme && pnpm run index:commands",
    "prepublishOnly": "pnpm run lint && pnpm run build",
    "release": "pnpm version patch --force && pnpm publish",
    "test": "cross-env NODE_DEBUG=matstack:* NODE_ENV=test node --loader ts-node/esm --enable-source-maps bin/test.ts",
    "typecheck": "tsc --noEmit",
    "index:commands": "adonis-kit index build/commands"
  },
  "dependencies": {
    "@poppinss/utils": "^6.10.1",
    "execa": "^9.6.0"
  },
  "devDependencies": {
    "@adonisjs/core": "^6.19.0",
    "@adonisjs/eslint-config": "^1.3.1",
    "@adonisjs/http-server": "^7.7.0",
    "@adonisjs/prettier-config": "^1.4.5",
    "@adonisjs/session": "^7.5.1",
    "@adonisjs/tsconfig": "^1.4.1",
    "@adonisjs/vite": "^4.0.0",
    "@japa/assert": "^3.0.0",
    "@japa/expect": "^3.0.6",
    "@japa/file-system": "^2.3.2",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.13.20",
    "@types/node": "^22.18.8",
    "@types/set-cookie-parser": "^2.4.10",
    "@types/supertest": "^6.0.3",
    "c8": "^10.1.3",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "del-cli": "^6.0.0",
    "eslint": "^8.57.1",
    "get-port": "^7.1.0",
    "node-mocks-http": "^1.17.2",
    "np": "^10.2.0",
    "prettier": "^3.6.2",
    "set-cookie-parser": "^2.7.1",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^6.3.6"
  },
  "peerDependencies": {
    "@react-router/node": "^7.9.0",
    "react-router": "^7.9.0"
  },
  "peerDependenciesMeta": {},
  "author": "Jarle Mathiesen",
  "license": "MIT",
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jarle/react-adonisjs"
  },
  "bugs": {
    "url": "https://github.com/jarle/react-adonisjs/issues"
  },
  "homepage": "https://matstack.dev/react-adonisjs",
  "prettier": "@adonisjs/prettier-config",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "np": {
    "message": "chore(release): %s",
    "tag": "latest",
    "branch": "main",
    "anyBranch": false
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  }
}

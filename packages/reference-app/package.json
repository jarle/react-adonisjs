{
  "name": "reference-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start": "node bin/server.js",
    "build": "remix build && node ace build",
    "dev": "PORT=3000 remix dev --manual -c \"node ace serve --watch\"",
    "test": "remix build && node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^6.1.3-29",
    "@adonisjs/eslint-config": "^1.2.0",
    "@adonisjs/prettier-config": "^1.2.0",
    "@adonisjs/tsconfig": "^1.2.0",
    "@japa/assert": "^2.1.0",
    "@japa/plugin-adonisjs": "^2.0.1",
    "@japa/runner": "^3.1.1",
    "@remix-run/dev": "^2.6.0",
    "@swc/core": "^1.4.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.54.0",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.1",
    "typescript": "5.3.3",
    "vite": "^5.0.2"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.1.1",
    "@adonisjs/core": "^6.2.3",
    "@adonisjs/lucid": "^19.0.0-5",
    "@adonisjs/session": "^7.0.0-14",
    "@adonisjs/shield": "^8.1.1",
    "@adonisjs/static": "^1.1.1",
    "@adonisjs/vite": "^2.0.2",
    "@japa/api-client": "^2.0.2",
    "@japa/browser-client": "^2.0.2",
    "@matstack/remix-adonisjs": "*",
    "@remix-run/css-bundle": "^2.6.0",
    "@remix-run/node": "^2.6.0",
    "@remix-run/react": "^2.6.0",
    "@remix-run/serve": "^2.6.0",
    "@vinejs/vine": "^1.7.1",
    "edge.js": "^6.0.0",
    "isbot": "^3.7.1-deprecated",
    "luxon": "^3.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.2.1",
    "sqlite3": "^5.1.7"
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/app"
  },
  "prettier": "@adonisjs/prettier-config"
}
